<SCENE>

<!-- Initial values -->
<INITIALS>
				<frustum near="0.1" far="500"/>
				<translation x="-1" y="-2" z="-4" />
				<rotation axis="x" angle="-15" />
				<rotation axis="y" angle="-15" />
				<rotation axis="z" angle="15" />
				<scale sx="0.15" sy="0.15" sz="0.15" />
				<reference length="5" />                        
</INITIALS>

<!-- Illumination and clear colour -->
<ILLUMINATION>
		<ambient r="0.4" g="0.4" b="0.4" a="1" />       
		<background r="0.2" g="0.6" b="0.8" a="1" />
</ILLUMINATION>

<!-- Lights -->
<LIGHTS>    
		<LIGHT id="exterior light">
				<enable value ="0" />                              
				<position  x="40" y="50" z="20" w="1" />
				<ambient r="0.6" g="0.5" b="0.4" a="1" />
				<diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
				<specular r="0.5" g="0.5" b="0.5" a="1.0" />
		</LIGHT>

		<LIGHT id="fleet light 1">                                    
				<enable value ="1" />                              
				<position  x="-20" y="0.2" z="-5" w="1" />             
				<ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
				<diffuse r="0.3" g="0.3" b="0.3" a="1.0" />        
				<specular r="0.7" g="0.7" b="0.7" a="1" />      
		</LIGHT>

		<LIGHT id="fleet light 2">                                    
				<enable value ="1" />                                        
				<position  x="-20" y="0.2" z="20" w="1" />             
				<ambient r="0.5" g="0.5" b="0.5" a="1.0" />          
				<diffuse r="0.3" g="0.3" b="0.3" a="1.0" />        
				<specular r="0.7" g="0.7" b="0.7" a="1" />      
		</LIGHT>

		<LIGHT id="fleet light 3">                                    
				<enable value ="1" />                              
				<position  x="-20" y="0.2" z="45" w="1" />             
				<ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
				<diffuse r="0.3" g="0.3" b="0.3" a="1.0" />        
				<specular r="0.7" g="0.7" b="0.7" a="1" />  
		</LIGHT>
</LIGHTS>

<TEXTURES>
		<!-- Glass Texture -->
		<TEXTURE id="glass">
				<file path="myimages/glass.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>	

		<!-- Wood Texture -->
		<TEXTURE id="wood">
				<file path="myimages/wood.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Steel Texture -->
		<TEXTURE id="steel">
				<file path="myimages/steel.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Steel Texture for the helix -->
		<TEXTURE id="helix steel">
				<file path="myimages/steel.jpg" />               
				<amplif_factor s="1" t="2" />                           
		</TEXTURE>

		<!-- Airplane design Texture 1 -->
		<TEXTURE id="design1">
				<file path="myimages/camouflage.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Airplane design Texture 2 -->
		<TEXTURE id="design2">
				<file path="myimages/usa.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Airplane design Texture 3 -->
		<TEXTURE id="design3">
				<file path="myimages/wood.jpg" />               
				<amplif_factor s="1" t="3" />                           
		</TEXTURE>

		<!-- Airplane design Texture 4 -->
		<TEXTURE id="design4">
				<file path="myimages/pattern.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Side wheel Texture-->
		<TEXTURE id="wheel">
				<file path="myimages/wheel.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

		<!-- Grass Texture-->
		<TEXTURE id="grass">
				<file path="myimages/grass.jpg" />               
				<amplif_factor s="20" t="20" />                           
		</TEXTURE>

		<!-- Asphalt Texture-->
		<TEXTURE id="asphalt">
				<file path="myimages/asphalt.jpg" />               
				<amplif_factor s="5" t="5" />                           
		</TEXTURE>

		<!-- Runaway Texture-->
		<TEXTURE id="runaway">
				<file path="myimages/runaway.jpg" />               
				<amplif_factor s="25" t="12" />                           
		</TEXTURE>

		<!-- Wharehouse Texture-->
		<TEXTURE id="wharehouse">
				<file path="myimages/wharehouse.jpg" />               
				<amplif_factor s="0.8" t="1" />                           
		</TEXTURE>

		<!-- Window Texture-->
		<TEXTURE id="window">
				<file path="myimages/window.jpg" />               
				<amplif_factor s="1" t="1" />                           
		</TEXTURE>

</TEXTURES>

<MATERIALS>
		<!-- Default material -->
		<MATERIAL id="defaultMaterial">                                  
				<shininess value="1" />
				<specular r="0" g="0" b="0" a="1" />        
				<diffuse r="1.0" g="1.0" b="1.0" a="1" />        
				<ambient r="0.2" g="0.2" b="0.2" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL>

		<!-- Glass Material -->
		<MATERIAL id="glass">                                  
				<shininess value="1" />
				<specular r="0.7" g="0.7" b="0.7" a="1" />        
				<diffuse r="0" g="0" b="0" a="1" />        
				<ambient r="0.8" g="0.8" b="1" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL>

		<!-- Wood Material -->
		<MATERIAL id="wood">                                  
				<shininess value="1" />
				<specular r="0.1" g="0.1" b="0.1" a="1" />        
				<diffuse r="0.3" g="0.3" b="0.3" a="1" />        
				<ambient r="0.5" g="0.5" b="0.5" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL>

		<!-- steel Material -->
		<MATERIAL id="steel">                                  
				<shininess value="1" />
				<specular r="0.4" g="0.4" b="0.4" a="1" />        
				<diffuse r="0.7" g="0.7" b="0.7" a="1" />        
				<ambient r="0.3" g="0.3" b="0.3" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL>    

		<!-- fan Material -->
		<MATERIAL id="fan">                                  
				<shininess value="1" />
				<specular r="0.5" g="0.5" b="0.5" a="1" />        
				<diffuse r="0" g="0" b="0" a="1" />        
				<ambient r="0.3" g="0.3" b="0.3" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL>    

		<!-- Wheel Material -->
		<MATERIAL id="wheel">                                  
				<shininess value="1" />
				<specular r="0" g="0" b="0" a="1" />        
				<diffuse r="0" g="0" b="0" a="1" />        
				<ambient r="1" g="1" b="1" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL> 

		<!-- Hihglight Material for non specular surfaces-->
		<MATERIAL id="highlight">                                  
				<shininess value="1" />
				<specular r="0" g="0" b="0" a="1" />        
				<diffuse r="0.8" g="0.8" b="0.8" a="1" />        
				<ambient r="0.8" g="0.8" b="0.8" a="1" />         
				<emission r="0.0" g="0.0" b="0.0" a="1" />        
		</MATERIAL> 

</MATERIALS>

<ANIMATIONS>

	<!-- Animation for opening the gate left door -->
	<ANIMATION id="open_left_door" speed="0.02" type="linear">
		<controlpoint xx="0.11" yy="0" zz="0"/>
		<controlpoint xx="-0.12" yy="0" zz="0"/>
	</ANIMATION>

	<!-- Animation for opening the gate right door -->
	<ANIMATION id="open_right_door" speed="0.02" type="linear">
		<controlpoint xx="-0.11" yy="0" zz="0"/>
		<controlpoint xx="0.12" yy="0" zz="0"/>
	</ANIMATION>

	<!-- Animation for making the airplaine's helix spin -->
	<ANIMATION id="fan_spin" speed="0.1" type="circular"
		centerx="0" centery="0" centerz="0"
		radius="0.04" startang="0" rotang="7200000"/>

	<!-- Animation for slow airplane speed -->
	<ANIMATION id="plane_slow_speed" speed="4" type="linear">
				<controlpoint xx="0" yy="-5.5" zz="-80"/>
				<controlpoint xx="0" yy="-5.5" zz="-65"/>
	</ANIMATION>

	<!-- Animation for medium airplane speed -->
	<ANIMATION id="plane_medium_speed" speed="8" type="linear">
				<controlpoint xx="0" yy="-5.5" zz="-65"/>
				<controlpoint xx="0" yy="-5.5" zz="-40"/>
	</ANIMATION>

	<!-- Animation for medium airplane speed -->
	<ANIMATION id="plane_high_speed" speed="12" type="linear">
				<controlpoint xx="0" yy="-5.5" zz="-40"/>
				<controlpoint xx="0" yy="-5.5" zz="-10"/>
	</ANIMATION>

	<!-- Airplane's track acceleration -->
	<ANIMATION id="plane_track_accelerate" type="combo">
		<SPANREF id="plane_slow_speed"/>
				<SPANREF id="plane_medium_speed"/>
		<SPANREF id="plane_high_speed"/>
		</ANIMATION>	

	<!-- Animation that makes the airplane leave the ground : part1 -->
	<ANIMATION id="leave_ground_1" speed="12" type="bezier">
		<controlpoint xx="0" yy="-5.5" zz="-10"/>
		<controlpoint xx="0" yy="-5.5" zz="5"/>
		<controlpoint xx="0" yy="2" zz="20"/>
		<controlpoint xx="0" yy="7" zz="30"/>
	</ANIMATION>

	<!-- Animation that makes the airplane leave the ground : part2 -->
	<ANIMATION id="leave_ground_2" speed="14" type="bezier">
		<controlpoint xx="0" yy="7" zz="30"/>
		<controlpoint xx="0" yy="12" zz="40"/>
		<controlpoint xx="10" yy="20" zz="50"/>
		<controlpoint xx="20" yy="20" zz="50"/>
	</ANIMATION>

	<!-- Animation that makes the airplane leave the ground : part3 -->
	<ANIMATION id="leave_ground_3" speed="14" type="bezier">
		<controlpoint xx="20" yy="20" zz="50"/>
		<controlpoint xx="30" yy="20" zz="50"/>
		<controlpoint xx="50" yy="20" zz="40"/>
		<controlpoint xx="50" yy="20" zz="30"/>
	</ANIMATION>

	<!-- Animation that makes the airplane leave the ground -->
	<ANIMATION id="leave_ground"  type="combo">
		<SPANREF id="leave_ground_1" />
		<SPANREF id="leave_ground_2" />
		<SPANREF id="leave_ground_3" />
	</ANIMATION>

	<!-- Animation for making the Airplane fly in loops : part1 -->
	<ANIMATION id="fly_loop_1" speed="17" type="linear">
		<controlpoint xx="50" yy="20" zz="30"/>
		<controlpoint xx="50" yy="20" zz="-40"/>
	</ANIMATION>

	<!-- Animation for making the Airplane fly in loops : part2 -->
	<ANIMATION id="fly_loop_2" speed="17" type="bezier">
		<controlpoint xx="50" yy="20" zz="-40"/>
		<controlpoint xx="50" yy="20" zz="-70"/>
		<controlpoint xx="-50" yy="20" zz="-70"/>
		<controlpoint xx="-50" yy="20" zz="-40"/>
	</ANIMATION>

	<!-- Animation for making the Airplane fly in loops : part3 -->
	<ANIMATION id="fly_loop_3" speed="17" type="linear">
		<controlpoint xx="-50" yy="20" zz="-40"/>
		<controlpoint xx="-50" yy="20" zz="30"/>
	</ANIMATION>

	<!-- Animation for making the Airplane fly in loops : part4 -->
	<ANIMATION id="fly_loop_4" speed="17" type="bezier">
		<controlpoint xx="-50" yy="20" zz="30"/>
		<controlpoint xx="-50" yy="20" zz="60"/>
		<controlpoint xx="50" yy="20" zz="60"/>
		<controlpoint xx="50" yy="20" zz="30"/>
	</ANIMATION>

	<!-- Animation for making the Airplane fly in loops -->
	<ANIMATION id="fly_loop" type="combo">
		<SPANREF id="fly_loop_1"/>
		<SPANREF id="fly_loop_2"/>
		<SPANREF id="fly_loop_3"/>
		<SPANREF id="fly_loop_4"/>
		</ANIMATION>

	<!-- Makes the object wait to seconds almost still -->
	<ANIMATION id="wait_2s" speed="0.0005" type="linear">
		<controlpoint xx="0" yy="0" zz="0"/>
		<controlpoint xx="0.001" yy="0" zz="0"/>
	</ANIMATION>

	<!-- Airplane moving to track -->
	<ANIMATION id="move_to_track" speed="4" type="linear">
		<controlpoint xx="0" yy="0" zz="0"/>
		<controlpoint xx="0" yy="0" zz="20"/>
		<controlpoint xx="40" yy="0" zz="60"/>
		<controlpoint xx="40" yy="0" zz="72"/>
	</ANIMATION>

	<!-- Animation for making the plane line up with the track -->
	<ANIMATION id="line_with_track" speed="4" type="circular"
		centerx="35" centery="0" centerz="72"
		radius="5" startang="0" rotang="-90" />
	
	<!-- Make the airplane traverse the track -->
	<ANIMATION id="traverse_track" speed="8" type="linear">
		<controlpoint xx="35" yy="0" zz="77"/>
		<controlpoint xx="-70" yy="0" zz="77"/>
	</ANIMATION>

	<!-- Animation for making the plane line up with the track -->
	<ANIMATION id="line_with_track_2" speed="4" type="circular"
		centerx="-70" centery="0" centerz="79"
		radius="2" startang="270" rotang="180" />


</ANIMATIONS>

<!-- The leaves -->
<LEAVES>
	
</LEAVES>

<!-- The nodes -->
<NODES>

		<ROOT id="root" />

		<!-- The root node -->
		<NODE id="root">
				<MATERIAL id="defaultMaterial" />
				<TEXTURE id="null" />

				<TRANSLATION x="20" y="0" z="20" />

				<DESCENDANTS>
					<NODEREF id="ground elements" />
					<NODEREF id="fleet" />
					<NODEREF id="landing airplane" />
					<NODEREF id="hangar" />
				</DESCENDANTS>

		</NODE>

		<!-- The fleet -->
		<NODE id="fleet">
				<MATERIAL id="null" />
				<TEXTURE id="null" />

				<TRANSLATION x="-50" y="4.2" z="25" />	
				<ROTATION axis="y" angle="90" />

				<DESCENDANTS>
					<NODEREF id="airplane 1" />
					<NODEREF id="airplane 2" />
					<NODEREF id="airplane 3" />
			</DESCENDANTS>

		</NODE>

		<!-- Airplane 1 from the fleet -->
		<NODE id="airplane 1">
				<MATERIAL id="null" />
				<TEXTURE id="design1" />

				<DESCENDANTS>
					<NODEREF id="airplane" />
				</DESCENDANTS>

		</NODE>

		<!-- Airplane 2 from the fleet -->
		<NODE id="airplane 2">       
				<MATERIAL id="null" />         
				<TEXTURE id="design2" /> 

				<TRANSLATION x="25" y="0" z="0" />

				<ANIMATIONREFS>
					<ANIMATIONREF id="wait_2s" />
					<ANIMATIONREF id="move_to_track" />
					<ANIMATIONREF id="line_with_track" />
					<ANIMATIONREF id="traverse_track" />
					<ANIMATIONREF id="line_with_track_2" />
				</ANIMATIONREFS>

				<DESCENDANTS>
					<NODEREF id="airplane" />
				</DESCENDANTS>

		</NODE>

		<!-- Airplane 3 from the fleet -->
		<NODE id="airplane 3">       
				<MATERIAL id="null" />         
				<TEXTURE id="design3" />

				<TRANSLATION x="50" y="0" z="0" /> 

				<DESCENDANTS>
					<NODEREF id="airplane" />
				</DESCENDANTS>

		</NODE>

		<!-- The landing airplane node -->
		<NODE id="landing airplane">       
				<MATERIAL id="defaultMaterial" />         
				<TEXTURE id="design1" /> 

				
				<TRANSLATION x="30" y="10" z="30" />				

				<ANIMATIONREFS>
					<ANIMATIONREF id="plane_track_accelerate" />
					<ANIMATIONREF id="leave_ground" />
					<ANIMATIONREF id="fly_loop" />
					<!-- <ANIMATIONREF id="fly_loop" /> -->
				</ANIMATIONREFS>

				<DESCENDANTS>
					<NODEREF id="airplane" />
				</DESCENDANTS>

		</NODE>

		<!-- A generic airplane - change design for different styles -->
		<NODE id="airplane">       
				<MATERIAL id="defaultMaterial" />         
				<TEXTURE id="null" />

				<SCALE sx="2" sy="2" sz="2" />  

				<DESCENDANTS>
					<NODEREF id="wings" />
					<NODEREF id="body" />
					<NODEREF id="wheels" /> 
					<NODEREF id="fan" />
					<NODEREF id="tail" />
				</DESCENDANTS>

		</NODE>

	<!-- *********************************** WINGS ******************************************* -->

		<!-- The wings -->
	 <NODE id="wings">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

			<DESCENDANTS>
			<NODEREF id="up wing" />
			<NODEREF id="down wing" />
			<NODEREF id="wing support" />
			</DESCENDANTS>
		
	 </NODE>

	 <!-- The up wing -->
	 <NODE id="up wing">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
		
		<TRANSLATION x="0" y="0.8" z="0" />

			<DESCENDANTS>
			<NODEREF id="wing" />
			</DESCENDANTS>
		
	 </NODE>

			<!-- The down wing -->
	 <NODE id="down wing">
			<MATERIAL id="null" />
			<TEXTURE id="null" />
		
		<TRANSLATION x="0" y="-0.8" z="0" />

			<DESCENDANTS>
			<NODEREF id="wing" />
			</DESCENDANTS>
		
	 </NODE>

	 <!-- A generic wing -->
	 <NODE id="wing">
			<MATERIAL id="null" />
			<TEXTURE id="null" />

		<SCALE sx="8.10" sy="0.1" sz="2.88" />
		
