<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.1" far="500"/>                 
    <translation x="-1" y="1" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="1" sy="1" sz="1" />         
    <reference length="5" />                        
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />       
    <background r="0.2" g="0.6" b="0.8" a="1" />          
</ILLUMINATION>

<!-- Lights -->
<LIGHTS>    
    <LIGHT id="light0">                                    
        <enable value ="1" />                              
        <position  x="0" y="10" z="0" w="1" />             
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />        
        <specular r="0.5" g="0.5" b="0.5" a="1.0" />      
    </LIGHT>

    <LIGHT id="light1">                                    
        <enable value ="1" />                              
        <position  x="10" y="0" z="15" w="1" />             
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />          
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />      
    </LIGHT>
</LIGHTS>

<TEXTURES>
    <!-- Tree trunk -->
    <TEXTURE id="trunk">
        <file path="images/trunk-image.jpg" />               
        <amplif_factor s="2" t="2" />                           
    </TEXTURE>	
</TEXTURES>

<MATERIALS>
    <!-- Default material -->
    <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0" g="0" b="0" a="1" />        
        <diffuse r="1.0" g="1.0" b="1.0" a="1" />        
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>
</MATERIALS>

<!-- The leaves -->
<LEAVES>
	
</LEAVES>

<!-- The nodes -->
<NODES>

    <ROOT id="root" />     

    <!-- The root node -->
    <NODE id="root">       
        <MATERIAL id="defaultMaterial" />         
        <TEXTURE id="clear" />  

        <DESCENDANTS>
			<NODEREF id="wings" />
			<NODEREF id="body" /> <!--
			<NODEREF id="wheels" /> 
			<NODEREF id="fan" />
			<NODEREF id="tail" />
			 -->
        </DESCENDANTS>

    </NODE>

    <!-- The wings -->
   <NODE id="wings">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

   		<DESCENDANTS>
			<NODEREF id="up wing" />
			<NODEREF id="down wing" />
			<NODEREF id="wing support" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- The up wing -->
   <NODE id="up wing">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
		
		<TRANSLATION x="0" y="0.8" z="0" />

   		<DESCENDANTS>
			<NODEREF id="wing" />
   		</DESCENDANTS>
   	
   </NODE>

      <!-- The down wing -->
   <NODE id="down wing">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
		
		<TRANSLATION x="0" y="-0.8" z="0" />

   		<DESCENDANTS>
			<NODEREF id="wing" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- A generic wing -->
   <NODE id="wing">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<SCALE sx="8.10" sy="0.1" sz="2.88" />
		
   		<DESCENDANTS>
   			<NODEREF id="cube" />
		</DESCENDANTS>
   	
   </NODE> 

   <!-- Wing's Support -->
   <NODE id="wing support">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder vertical1" />
   			<NODEREF id="holder vertical2" />
   			<NODEREF id="holder vertical3" />
   			<NODEREF id="holder vertical4" />
   			<NODEREF id="holder inclined1" />
   			<NODEREF id="holder inclined2" />
   			<NODEREF id="holder inclined3" />
   			<NODEREF id="holder inclined4" />
   			<NODEREF id="holder middle1" />
   			<NODEREF id="holder middle2" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder vertical1 -->
   <NODE id="holder vertical1">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="3" y="0" z="0.9" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder vertical1 -->
   <NODE id="holder vertical2">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="3" y="0" z="-0.9" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE> 

   <!-- holder vertical1 -->
   <NODE id="holder vertical3">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-3" y="0" z="0.9" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE> 

   <!-- holder vertical1 -->
   <NODE id="holder vertical4">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-3" y="0" z="-0.9" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE> 

   <!-- holder inclined1 -->
   <NODE id="holder inclined1">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-0.5" y="0.15" z="0.8" />
		<ROTATION axis="x" angle="20" />
		<ROTATION axis="z" angle="30" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder inclined2 -->
   <NODE id="holder inclined2">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-0.5" y="0.15" z="-0.8" />
		<ROTATION axis="x" angle="-20" />
		<ROTATION axis="z" angle="30" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder inclined3 -->
   <NODE id="holder inclined3">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0.5" y="0.15" z="0.8" />
		<ROTATION axis="x" angle="20" />
		<ROTATION axis="z" angle="-30" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder inclined4 -->
   <NODE id="holder inclined4">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0.5" y="0.15" z="-0.8" />
		<ROTATION axis="x" angle="-20" />
		<ROTATION axis="z" angle="-30" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder middle1 -->
   <NODE id="holder middle1">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-1.6" y="0" z="0" />
		<ROTATION axis="z" angle="-17" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- holder middle2 -->
   <NODE id="holder middle2">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="1.6" y="0" z="0" />
		<ROTATION axis="z" angle="17" />
		
   		<DESCENDANTS>
   			<NODEREF id="holder" />
		</DESCENDANTS>
   	
   </NODE>

   <!-- Generic holder -->
   <NODE id="holder">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="0.8" z="0" />
		<ROTATION axis="x" angle="90" />
		
   		<DESCENDANTS>
   			<LEAF type="cylinder" args="1.6 0.1 0.1 20 20" />
		</DESCENDANTS>
   	
   </NODE>    

   <!-- Airplane's Body -->
   <NODE id="body">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

   		<TRANSLATION x="0" y="0.15" z="0" />
		 
   		<DESCENDANTS>
   			<NODEREF id="main body" />
   			<NODEREF id="back body" />
   			<NODEREF id="cockpit" /> 
		</DESCENDANTS>
   	
   </NODE>

   <!-- Main Body -->
   <NODE id="main body">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="-0.7" z="-1.5"/> 
		
   		<DESCENDANTS>
            <LEAF type="cylinder" args="3.63 0.7 0.7 20 20" /> 
		</DESCENDANTS>
   	
   </NODE> 

   <!-- Cockpit -->
   <NODE id="back body">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		
		<TRANSLATION x="0" y="-0.7" z="-1.5" />
		<SCALE sx="0.7" sy="0.7" sz="2.63" />
				
   		<DESCENDANTS>
            <LEAF type="sphere" args="1 20 20" /> 
		</DESCENDANTS>
   	
   </NODE>

   <!-- Cockpit -->
   <NODE id="cockpit">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="-0.1" z="0.8" />
		<SCALE sx="1" sy="1" sz="2" />
				
   		<DESCENDANTS>
            <LEAF type="sphere" args="0.4 20 20" /> 
		</DESCENDANTS>
   	
   </NODE>

   <!-- wheels -->
<!--   <NODE id="wheels">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
				
   		<DESCENDANTS>
            <NODEREF id="wheel and support 1" /> 
            <NODEREF id="wheel and support 2" /> 
		</DESCENDANTS>
   	
   </NODE> -->

   <!-- UTILS -->

   <!-- Unit Cube -->
   <NODE id="cube">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
		
   		<DESCENDANTS>
			<NODEREF id="cube top" />
			<NODEREF id="cube front" />
			<NODEREF id="cube left" />
			<NODEREF id="cube right" />
			<NODEREF id="cube back" />
			<NODEREF id="cube under" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's top -->
   <NODE id="cube top">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="0.5" z="0" />
   		<ROTATION axis="x" angle="-90" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's front -->
   <NODE id="cube front">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="0.5" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's left -->
   <NODE id="cube left">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="-0.5" y="0" z="0" />
   		<ROTATION axis="y" angle="-90" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's right -->
   <NODE id="cube right">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0.5" y="0" z="0" />
   		<ROTATION axis="y" angle="90" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's back -->
   <NODE id="cube back">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="-0.5" />
   		<ROTATION axis="x" angle="180" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Cube's under -->
   <NODE id="cube under">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="-0.5" z="0" />
   		<ROTATION axis="x" angle="90" />
		
   		<DESCENDANTS>
			<LEAF type="rectangle" args="-0.5 0.5 0.5 -0.5" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- Generic covered cylinder with radius 1 and height 1 -->
   <NODE id="covered cylinder">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />
		
   		<DESCENDANTS>
			<LEAF type="cylinder" args="1 1 1 20 20" />
			<NODEREF id="cover circle1" />
			<NODEREF id="cover circle2" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- cover circle1 -->
   <NODE id="cover circle1">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

		<TRANSLATION x="0" y="0" z="1" />
   		<SCALE sx="1" sy="1" sz="0.01" />
		
   		<DESCENDANTS>
			<LEAF type="sphere" args="1 20 20" />
   		</DESCENDANTS>
   	
   </NODE>

   <!-- cover circle2 -->
   <NODE id="cover circle2">
   		<MATERIAL id="null" />
   		<TEXTURE id="null" />

   		<SCALE sx="1" sy="1" sz="0.01" />
		
   		<DESCENDANTS>
			<LEAF type="sphere" args="1 20 20" />
   		</DESCENDANTS>
   	
   </NODE>

</NODES>

</SCENE>